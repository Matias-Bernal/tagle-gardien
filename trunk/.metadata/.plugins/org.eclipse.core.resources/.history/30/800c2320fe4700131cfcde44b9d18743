													Calendar calendar = new GregorianCalendar(frf.getYear(), frf.getMonth()-1, frf.getDay());
													long diferencia = ( hoy.getTime() - frf.getTime() )/MILLSECS_PER_DAY;
													row.add(String.valueOf(diferencia));
												}else{
													row.add("");
													row.add("");
												}	
